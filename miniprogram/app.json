{
  "preloadRule": {
    "pages/home/home": {
      "packages": ["packageHome"],
      "network": "all"
    }
  },
  "subpackages": [
    {
      "root": "packageHome",
      "pages": [
        "pages/activity/activity",
        "pages/exam/exam",
        "pages/guidance/guidance",
        "pages/score/score"
      ]
    }
  ],
  "pages": [
    "pages/home/home",
    "pages/schedule/schedule",
    "pages/profile/profile",
    "pages/login/login"
  ],
  "window": {
    "navigationBarBackgroundColor": "#ff8198",
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "HAUTDAYS|河工大教务",
    "backgroundColor": "#eeeeee",
    "navigationStyle": "custom",
    "enablePullDownRefresh": false
  },
  "tabBar": {
    "custom": true,
    "color": "#000000",
    "selectedColor": "#194675",
    "backgroundColor": "transparent",
    "position": "bottom",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/home/home",
        "iconPath": "assets/tabBar/home.png",
        "selectedIconPath": "assets/tabBar/home.png"
      },
      {
        "pagePath": "pages/schedule/schedule",
        "iconPath": "assets/tabBar/schedule.png",
        "selectedIconPath": "assets/tabBar/schedule.png"
      },
      {
        "pagePath": "pages/profile/profile",
        "iconPath": "assets/tabBar/me.png",
        "selectedIconPath": "assets/tabBar/me.png"
      }
    ]
  },
  "sitemapLocation": "sitemap.json"
}
